{
  "name": "fanosity-clients",
  "version": "2.1.2",
  "private": true,
  "scripts": {
    "app:start": "npm run app:clean && node node_modules/react-native/local-cli/cli.js start",
    "app:clean": "rm -rf $TMPDIR/react-* && rm -rf /tmp/metro-bundler-cache-* && rm -rf /tmp/haste-map-react-native-packager-* && watchman watch-del-all",
    "app:bundle-android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "app:release": "cd android && ./gradlew assembleRelease",
    "web:start": "npm run web:clean && BUILD_TYPE=web webpack-dev-server --env.mode development --env.config local",
    "web:build": "npm run web:clean && BUILD_TYPE=web webpack -p --env.mode production",
    "web:clean": "rimraf ./dist",
    "web:deploy-dev": "npm run web:build && AWS_PROFILE=serverless-elm aws s3 sync ./dist/ s3://dev-www.fanosity.com --region=us-west-2 --delete --acl public-read",
    "web:deploy-prod": "npm run web:build && AWS_PROFILE=serverless-elm aws s3 sync ./dist/ s3://www.fanosity.com --region=us-west-2 --delete --acl public-read",
    "test": "NODE_ENV=test jest"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "@material-ui/lab": "^3.0.0-alpha.30",
    "@react-native-community/async-storage": "^1.3.4",
    "apollo-cache-inmemory": "^1.5.1",
    "apollo-cache-persist": "^0.1.1",
    "apollo-client": "^2.5.1",
    "apollo-link": "^1.2.11",
    "apollo-link-error": "^1.1.10",
    "apollo-link-http": "^1.5.14",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "firebase": "^5.8.6",
    "graphql": "^14.2.1",
    "graphql-tag": "^2.10.1",
    "hex-to-rgba": "^1.0.2",
    "lodash": "^4.17.11",
    "mui-datatables": "^2.0.0",
    "prop-types": "^15.7.2",
    "react": "16.8.3",
    "react-apollo": "^2.5.5",
    "react-dom": "^16.8.6",
    "react-image-crop": "^7.0.2",
    "react-native": "^0.59.8",
    "react-native-camera": "^2.6.0",
    "react-native-drawer": "^2.5.1",
    "react-native-image-cache-wrapper": "^1.0.4",
    "react-native-modal": "^11.0.1",
    "react-native-qrcode-scanner": "^1.1.2",
    "react-native-router-flux": "^4.0.6",
    "react-redux": "^6.0.1",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "rn-fetch-blob": "^0.10.15"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.4.4",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-import-redirect": "^1.1.1",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-webpack-alias": "^2.1.2",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^5.2.0",
    "css-hot-loader": "^1.4.4",
    "css-loader": "^2.1.1",
    "eslint": "^4.17.0",
    "eslint-config-cbtnuggets": "^8.0.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-class-property": "^1.0.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.22.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.6",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.7.1",
    "metro-react-native-babel-preset": "^0.54.0",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.17.1",
    "react-test-renderer": "16.8.3",
    "sass-loader": "^7.1.0",
    "source-list-map": "^2.0.1",
    "style-loader": "^0.23.1",
    "uglify-js": "^3.5.10",
    "url-loader": "^1.1.2",
    "webpack": "^4.30.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.2",
    "webpack-core": "^0.6.9",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1",
    "write-file-webpack-plugin": "^4.5.0"
  },
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-navigation|@react-navigation|@react-native-community|react-navigation-(tabs|drawer|deprecated-tab-navigator|stack))"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|png|svg|pdf)$": "<rootDir>/tests/mockModule.js",
      "\\.(s*)css$": "identity-obj-proxy"
    }
  }
}
